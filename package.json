  "scripts": {
    "c:n:w": "if exist node_modules rmdir /Q /S node_modules",
    "c:n:u": "rm -rf node_modules",
    "c:l:w": "if exist package-lock.json del /F /Q package-lock.json",
    "c:l:u": "rm -f package-lock.json",
    "install:windows": "npm run c:n:w && npm run c:l:w && npm i --legacy-peer-deps",
    "install:unix": "npm run c:n:u && npm run c:l:u && npm i --legacy-peer-deps",
    "start": "react-scripts start",
    "start:sta": "cross-env PUBLIC_URL=/sta react-scripts start",
    "build": "react-scripts build",
    "build:sta": "cross-env PUBLIC_URL=/sta react-scripts build",
    "rn:dev": "rename build build-dev",
    "rn:sta:win": "rename build build-sta",
    "clean:dev": "if exist build-dev rmdir /Q /S build-dev",
    "clean:sta:win": "if exist build-sta rmdir /Q /S build-sta",
    "build:dev": "npm run build && npm run clean:dev && npm run rn:dev",
    "build:sta:win": "npm run build:sta && npm run clean:sta:win && npm run rn:sta:win",
    "build:all:win": "npm run build:dev && npm run build:sta:win",
    "rn:unix": "mv build build-dev",
    "rn:sta:unix": "mv build build-sta",
    "clean:unix": "rm -rf build-dev",
    "clean:sta:unix": "rm -rf build-sta",
    "build:dev:unix": "npm run build && npm run clean:unix && npm run rn:unix",
    "build:sta:unix": "npm run build:sta && npm run clean:sta:unix && npm run rn:sta:unix",
    "build:all:unix": "npm run build:dev:unix && npm run build:sta:unix",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all",
      "not chrome <51",
      "not safari <10",
      "ie 11"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version",
      "ie 11"
    ]
  },
  "devDependencies": {
    "@types/http-proxy": "^1.17.7",
    "@types/lodash": "^4.14.179",
    "@types/react-file-icon": "^1.0.1",
